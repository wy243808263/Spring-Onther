function InitAreaParent(){
   jQuery.post('/admin/area_level','ver='+new Date().getTime()+'&area.level=1',function(data){
   jQuery('#parent').html('').append(data);
		});
 }
 
 function selArea(id,level,name){
   jQuery('#pid').val(id);
   jQuery('#level').val(level);
   jQuery('#pname').val(name);
 }

 