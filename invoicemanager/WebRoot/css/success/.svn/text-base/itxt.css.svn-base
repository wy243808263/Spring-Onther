
@import url("global.css");
body {
	/*background: #f1f1f1;*/
	width: 100%
}
.itxt {
	width: 1190px;
	margin: 0 auto 30px;
	overflow: hidden
}
.iwrapper {
	background: #fff;
	position: relative;
	min-height:1315px;
}
.itit {
	position: relative;
	height: 70px;
	overflow: hidden;
	margin-top: -20px
}
.itit .toback {
	position: absolute;
	right: 0;
	top: 20px;
	z-index: 5;
	float: left;
	height: 30px;
	line-height: 30px;
	width: 60px;
	background: #e7e7e7;
	text-decoration: none;
	text-align: center;
	color: #475058;
	text-shadow: 0 -1px 0 #fff;
	border-radius: 2px
}
.itit .toback i {
	font-family: simsun;
	color: #666;
	font-weight: 700;
	font-style: normal;
	font-size: 14px;
	margin-right: 5px
}
.itit .toback:hover {
	background: #ddd
}
.itit .toback .icons {
	position: absolute;
	left: 10px;
	top: 10px
}
.itit .line {
	height: 1px;
	font-size: 0;
	line-height: 0;
	overflow: hidden;
	float: left;
	background: #e7ecee;
	width: 100%;
	position: absolute;
	left: 0;
	top: 35px
}
.itit h2, .itit .r {
	font-size: 16px;
	height: 30px;
	line-height: 30px;
	color: #fff;
	position: absolute;
	left: 20px;
	top: 20px;
	z-index: 3;
	padding: 0 15px;
	background: #0d95dd
}
.itit .r {
	font-size: 14px;
	color: #888;
	left: auto;
	right: 30px
}
.itit .r span {
	background: #1bb8e2;
	padding: 0 8px;
	border-radius: 2px
}
.itit .r a {
	color: #fff
}
.backtohis .toback {
	left: 0;
	right: auto
}
.backtohis h2 {
	left: 60px
}
.pagesidebar {
	position: absolute;
	top: 0;
	left: 0;
	width: 200px;
	background-color: #fbfbfb;
	bottom: 0
}
.pagesidebar ul {
	font-size: 14px;
	border-bottom: 1px solid #eee;
	margin-top: -2px
}
.pagesidebar ul li a {
	margin-bottom: 1px;
	padding: 10px 15px 10px 45px;
	color: #999;
	display: block;
	border-top: 1px dashed #eee
}
.pagesidebar ul li.cur a, .pagesidebar ul li a:hover {
	background: #fff;
	color: #12adff
}
.pagesidebar h4 a {
	margin-bottom: 1px;
	padding: 13px 15px;
	color: #666;
	border-left: solid 4px transparent;
	cursor: pointer;
	overflow: hidden;
	border-bottom: 1px solid #eee;
	display: block;
	text-decoration: none
}
.pagesidebar h4 a:hover, .pagesidebar h4.cur a, .pagesidebar h4.cur a {
	color: #333;
	background-color: #fff;
	border-left-color: ##12adff;;
	margin-right: -1px;
	position: relative;
	z-index: 2
}
.pagecontent {
	margin-left: 200px;
	min-height: 1315px;
	border-left: 1px solid #eee;
	padding: 15px 20px 20px 20px;
	line-height: 30px
}
.pagecontent a {
	color: #39f
}
.pagecontent p {
	margin-bottom: 10px;
	color: #676767
}
.article-content section {
	overflow: hidden
}
.article-content img {
	max-width: 100%;
	height: auto;
	vertical-align: top
}
.muted {
	clear: left;
	display: block
}
.focus {
	width: 200px;
	height: 120px;
	float: left;
	display: inline;
	margin-right: 20px;
	overflow: hidden
}
.focus img {
	width: 200px;
	height: 100%
}
.text {
	margin-bottom: 25px
}
.text dl {
	font-size: 14px;
	color: #676767;
	margin: 8px 0;
	padding: 8px 0;
	background-color: #f7f7f7;
	overflow: hidden
}
.text dl.first {
	background: #fff
}
.text dl dt {
	float: left;
	width: 20%;
	color: #676767;
	color: #000;
	font-size: 12px;
	padding-left: 2%
}
.text dl dd h4 {
	color: #333;
	margin-bottom: 10px
}
.text dl dd {
	width: 70%;
	float: right;
	padding-right: 2%;
	line-height: 26px
}
.text dl dd a {
	color: #676767
}
hr {
	border: 1px dashed #eee;
	margin: 15px 0;
	clear: both
}
.partners dl {
	overflow: hidden;
	clear: both
}
.partners dt {
	float: left;
	display: inline;
	width: 30%;
	text-align: center;
	border: 1px solid #eee;
	padding: 2px
}
.partners dt img {
	display: block
}
.partners dd {
	float: right;
	width: 67%
}
@media only screen and (min-width:1200px)and (max-width:1280px) {
.itxt {
	width: 1000px
}
.pagesidebar {
	width: 170px
}
.pagecontent {
	margin-left: 170px
}
}
.help_banner {
	position: relative;
	height: 230px;
	overflow: hidden;
	background: url('../images/slider/loading.gif') 50% no-repeat
}
.slides {
	position: relative;
	z-index: 1
}
.slides li {
	height: 230px;
	width: 100%;
	max-height: 240px
}
.flex-control-nav {
	position: absolute;
	bottom: 5px;
	z-index: 2;
	width: 100%;
	text-align: center
}
.flex-control-nav li {
	display: inline-block;
	width: 14px;
	height: 14px;
	margin: 0 5px;
*display:inline;
	zoom: 1
}
.flex-control-nav a {
	border-radius: 50%;
	display: inline-block;
	width: 14px;
	height: 14px;
	line-height: 40px;
	overflow: hidden;
	background: #fff;
	cursor: pointer
}
.flex-control-nav .flex-active {
	background: #f60
}
.flex-direction-nav {
	position: absolute;
	z-index: 3;
	width: 100%;
	top: 45%
}
.flex-direction-nav li a {
	display: block;
	width: 50px;
	height: 33px;
	overflow: hidden;
	cursor: pointer;
	position: absolute
}
.flex-direction-nav li a.flex-prev {
	left: 20px;
	background: url('../images/slider/prev.png') center center no-repeat
}
.flex-direction-nav li a.flex-next {
	right: 20px;
	background: url('../images/slider/next.png') center center no-repeat
}
